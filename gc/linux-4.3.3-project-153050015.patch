+#include<linux/module.h>
+#include<linux/string.h>
+#include<linux/init.h>
+#include<linux/fs.h>
+#include<linux/mm.h>
+#include<linux/slab.h>
+#include<asm/uaccess.h>
+
+MODULE_LICENSE("GPL");
+
+char *ptr,dev_name[20]="/dev/usermap",*tst_str="Kernel Header ";
+int dev_maj=89;
+
+static int user_open(struct inode *node,struct file* flp){
+	//open_count++;
+	printk("[dev mod] Device Opened\n");
+	return 0;
+}
+
+static ssize_t user_read(struct file *flp, char *buff, size_t len, loff_t *offset){
+	int ret=0,pos=0;
+	if(!(*offset)) 
+	while(ptr[pos]!=0){
+		put_user(ptr[pos++],buff++);
+		ret++; *offset=1;
+	}
+	return ret;
+}
+
+static ssize_t user_write(struct file *flp, const char *buff, size_t len, loff_t *off){
+	int pos=0;
+	while(pos<len){
+		ptr[pos]=buff[pos];
+		pos++;
+	}
+	return len;
+}
+
+static int user_release(struct inode *node, struct file *flp){
+	printk("[dev mod] Device Closed\n");
+	return 0;
+}
+
+static int user_fault(struct vm_area_struct *vm_struct, struct vm_fault *vmf){
+	printk("[dev mod] Fault Occoured at v-add: %lx\n",(long)(vmf->virtual_address));
+	return 0;
+}
+
+static struct vm_operations_struct vma_ops={
+	.fault=user_fault
+};
+
+static int user_mmap(struct file *flp, struct vm_area_struct *vma){
+	printk("[dev mod] mmap called | vma region[%lx-%lx] | ptr add: %lx\n",vma->vm_start,vma->vm_end,(long)ptr);
+
+	vma->vm_pgoff=(virt_to_phys(ptr)>>PAGE_SHIFT);
+	if(remap_pfn_range(vma, vma->vm_start, (vma->vm_pgoff), vma->vm_end-vma->vm_start, vma->vm_page_prot)) return -EAGAIN;
+
+	vma->vm_ops=&vma_ops;	
+	printk("[dev mod] remap success\n");	
+	return 0;
+}
+
+static struct file_operations f_ops={
+	.read=user_read,
+	.write=user_write,
+	.open=user_open,
+	.mmap=user_mmap,
+	.release=user_release
+};
+
+int init_module(void){
+	int i,ret;
+	ret=register_chrdev(dev_maj, dev_name,&f_ops);
+	
+	if(ret<0) printk("[dev mod] Dev Registration Problem %d\n",ret);
+	else printk("[dev mod] Dev Registered\n");	
+
+	ptr=(char*)kmalloc(PAGE_SIZE,GFP_KERNEL);
+	
+	for(i=0;i<strlen(tst_str);i++)
+		ptr[i]=tst_str[i];
+	ptr[i]='\0';
+
+	return ret;	
+}
+
+void cleanup_module(void){
+	int i=0;
+	printk("[dev mod] dump: ");
+	for(i=0;i<strlen(ptr);i++)
+		printk("%c",ptr[i]);
+	printk("\n");
+	unregister_chrdev(dev_maj,dev_name);
+}
